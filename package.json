{
    "name": "infusion-docs",
    "version": "1.5.0",
    "description": "Infusion Documentation",
    "license": "CC-BY-3.0",
    "repository": "https://github.com/fluid-project/infusion-docs",
    "scripts": {
        "start": "npm-run-all -l clean dev",
        "build": "npm-run-all -l clean build:*",
        "build:eleventy": "cross-env ELEVENTY_ENV=production eleventy",
        "clean": "rimraf _site",
        "dev": "cross-env ELEVENTY_ENV=development eleventy --serve",
        "lint": "fluid-lint-all",
        "pretest": "npm run build",
        "test": "run-s test:*",
        "test:node": "node tests/all-node-tests.js",
        "test:browser": "node node_modules/testem/testem.js ci --file tests/testem.js"
    },
    "dependencies": {
        "@11ty/eleventy": "2.0.1",
        "@11ty/eleventy-navigation": "0.3.5",
        "fluid-binder": "2.0.0",
        "fluid-location-bar-relay": "1.0.7",
        "foundation-sites": "5.5.2",
        "highlight.js": "11.8.0",
        "infusion": "4.6.0",
        "lunr": "2.3.9",
        "markdown-it": "13.0.1",
        "markdown-it-anchor": "8.6.7",
        "rimraf": "5.0.1",
        "string": "3.3.3"
    },
    "devDependencies": {
        "cross-env": "7.0.3",
        "fluid-express": "1.0.18",
        "fluid-lint-all": "1.2.9",
        "fluid-testem": "2.2.1",
        "github-slugger": "1.5.0",
        "jsdom": "22.1.0",
        "kettle": "2.3.0",
        "node-jqunit": "1.1.9",
        "npm-run-all": "4.1.5",
        "request": "2.88.2",
        "testem": "3.10.1"
    }
}
